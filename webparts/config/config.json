{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "banner-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/banner/BannerWebPart.js",
          "manifest": "./src/webparts/banner/BannerWebPart.manifest.json"
        }
      ]
    },
    "footer-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/footer/FooterWebPart.js",
          "manifest": "./src/webparts/footer/FooterWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "BannerWebPartStrings": "lib/webparts/banner/loc/{locale}.js",
    "FooterWebPartStrings": "lib/webparts/footer/loc/{locale}.js"
  }
}
